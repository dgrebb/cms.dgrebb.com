{
  "kind": "collectionType",
  "collectionName": "certifications",
  "info": {
    "singularName": "certification",
    "pluralName": "certifications",
    "displayName": "CV - Certifications",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "inversedBy": "certifications"
    },
    "body": {
      "type": "richtext"
    },
    "dates": {
      "displayName": "Certification Dates",
      "type": "component",
      "repeatable": true,
      "component": "cv.certification-dates"
    },
    "documentation": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "positions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::position.position",
      "mappedBy": "certifications"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "experiences": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::experience.experience",
      "inversedBy": "certifications"
    }
  }
}
