{
  "name": "dgrebb.com",
  "version": "3.11.1",
  "private": true,
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "prettier": "prettier",
    "baseline": "mv -f ./.report/bundle/current/* ./.report/bundle/previous",
    "bundle-stats": "bundle-stats",
    "lint": "prettier --check . && eslint .",
    "dev.l": "vite --config vite.local.config.js dev",
    "dev.s": "vite --config vite.local.config.js dev",
    "dev.p": "vite --config vite.local.config.js dev",
    "dev": "vite --config vite.config.js dev",
    "build.l": "WARN_ONLY=true vite --config vite.local.config.js build",
    "build.s": "vite --config vite.config.js build",
    "build.p": "vite --config vite.config.js build",
    "build": "vite --config vite.config.js build",
    "preview": "vite --config vite.config.js preview",
    "preview.local": "vite --config vite.local.config.js preview",
    "postbuild.l": "npx -y -q svelte-sitemap -i '404.html' -i '/404/index.html' -i 'shingle.html' -i '/shingle/index.html' -i '/posts/category/index.html' -i '**/cv/**' -t --attribution=false --domain https://local.dgrebb.com",
    "postbuild.s": "npx -y -q svelte-sitemap -i '404.html' -i '/404/index.html' -i 'shingle.html' -i '/shingle/index.html' -i '/posts/category/index.html' -i '**/cv/**' -t --attribution=false --domain https://stg.dgrebb.com",
    "postbuild": "npx -y -q svelte-sitemap -i '404.html' -i '/404/index.html' -i 'shingle.html' -i '/shingle/index.html' -i '/posts/category/index.html' -i '**/cv/**' -t --attribution=false --domain https://www.dgrebb.com",
    "format": "prettier --write .",
    "test.all": "npm run test.integration && npm run test.unit",
    "test.integration": "playwright test",
    "test.unit": "vitest",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@accuser/svelte-plausible-analytics": "dgrebb/svelte-plausible-analytics#downstream/feature/support-custom-pageview-properties",
    "@iconify-json/entypo-social": "^1.1.8",
    "@iconify-json/gg": "^1.1.9",
    "@iconify-json/icomoon-free": "^1.1.8",
    "@iconify-json/icon-park-outline": "^1.1.15",
    "@iconify-json/mdi": "^1.1.62",
    "@iconify-json/ph": "^1.1.9",
    "@iconify-json/simple-icons": "^1.1.84",
    "@iconify-json/typcn": "^1.1.9",
    "@playwright/test": "^1.40.1",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "html-minifier-terser": "^7.2.0",
    "marked": "^11.1.0",
    "marked-alert": "^2.0.1",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rollup-plugin-bundle-stats": "^4.8.3",
    "rollup-plugin-visualizer": "^5.11.0",
    "slugger": "^1.0.1",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-highlight": "7.4.2",
    "svelte-meta-tags": "^3.1.0",
    "svelte-sitemap": "^2.6.0",
    "typescript": "^5.3.3",
    "unplugin-icons": "^0.18.1",
    "vite": "^5.0.9",
    "vite-plugin-progress": "^0.0.7",
    "vitest": "^1.0.4"
  },
  "type": "module"
}
