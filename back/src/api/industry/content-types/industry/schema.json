      "inversedBy": "industries"
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "industries"
    },
    "body": {
      "type": "richtext"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "experiences": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::experience.experience",
      "mappedBy": "industries"
    }
