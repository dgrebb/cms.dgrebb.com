{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@api": ["./src/lib/api"],
      "@components": ["./src/lib/components"],
      "@components/*": ["./src/lib/components/*"],
      "@layout": ["./src/lib/layout"],
      "@layout/*": ["./src/lib/layout/*"],
      "@store": ["./src/lib/store"],
      "@styles": ["./src/lib/styles"],
      "@utils": ["./src/lib/_utils"],
      "@shape-shifters": ["./src/lib/_utils/shape-shifters"],
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"]
    }
  }
}
