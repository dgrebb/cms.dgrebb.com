{
  "kind": "collectionType",
  "collectionName": "industries",
  "info": {
    "singularName": "industry",
    "pluralName": "industries",
    "displayName": "CV - Industries",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
