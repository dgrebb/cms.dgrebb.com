{
  "kind": "collectionType",
  "collectionName": "positions",
  "info": {
    "singularName": "position",
    "pluralName": "positions",
    "displayName": "CV - Positions",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "title": {
      "type": "string"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "summary": {
      "type": "text"
    },
    "hero": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "organizations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "inversedBy": "positions"
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "positions"
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "inversedBy": "positions"
    },
    "industries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::industry.industry",
      "inversedBy": "positions"
    },
    "awards": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::award.award",
      "mappedBy": "positions"
    },
    "certifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::certification.certification",
      "inversedBy": "positions"
    },
    "accomplishments": {
      "displayName": "Accomplishments",
      "type": "component",
      "repeatable": true,
      "component": "cv.accomplishments"
    },
    "responsibilities": {
      "displayName": "Responsibilities",
      "type": "component",
      "repeatable": true,
      "component": "cv.responsibilities"
    },
    "wisdom": {
      "displayName": "Wisdom",
      "type": "component",
      "repeatable": true,
      "component": "cv.wisdom"
    },
    "body": {
      "type": "richtext"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
