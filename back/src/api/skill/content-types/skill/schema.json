        "software",
        "hardware",
        "planning",
        "language",
        "people",
        "management",
        "production"
      ]
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "skills"
    },
    "positions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::position.position",
