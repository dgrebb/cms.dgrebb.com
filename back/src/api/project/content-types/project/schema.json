{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "CV - Projects",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "hero": {
      "allowedTypes": ["images"],
      "type": "media",
      "multiple": false
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "URL": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "positions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::position.position",
      "inversedBy": "projects"
    },
    "industries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::industry.industry",
      "inversedBy": "projects"
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "inversedBy": "projects"
    },
    "organizations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "inversedBy": "projects"
    },
    "awards": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::award.award",
      "mappedBy": "projects"
    },
    "body": {
      "type": "richtext"
    },
    "artifacts": {
      "type": "dynamiczone",
      "components": [
        "artifacts.websites",
        "artifacts.open-source",
        "artifacts.videos",
        "artifacts.audio",
        "artifacts.images"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
