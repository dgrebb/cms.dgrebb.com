{
  "kind": "collectionType",
  "collectionName": "skills",
  "info": {
    "singularName": "skill",
    "pluralName": "skills",
    "displayName": "CV - Skills",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "skill": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "skill"
    },
    "proficiency": {
      "type": "integer",
      "max": 10,
      "required": true
    },
    "icon": {
      "type": "enumeration",
      "enum": [
        "vscode",
        "html",
        "css",
        "magento",
        "php",
        "drupal",
        "craftcms",
        "json",
        "api",
        "wordpress",
        "charts",
        "vagrant",
        "gulp",
        "sass",
        "less",
        "postcss",
        "wpengine",
        "pantheon",
        "codeship",
        "aws",
        "terraform",
        "javascript"
      ]
    },
    "summary": {
      "type": "text"
    },
    "classification": {
      "type": "enumeration",
      "enum": [
        "software",
        "hardware",
        "planning",
        "language",
        "people",
        "management",
        "production",
        "framework",
        "devops tools"
      ]
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "skills"
    },
    "positions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::position.position",
      "mappedBy": "skills"
    },
    "organizations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "mappedBy": "skills"
    },
    "body": {
      "type": "richtext"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
