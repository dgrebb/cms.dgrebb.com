{
  "kind": "collectionType",
  "collectionName": "awards",
  "info": {
    "singularName": "award",
    "pluralName": "awards",
    "displayName": "CV - Awards",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "URL": {
      "type": "string",
      "regex": "\\b(?:https?|ftp):\\/\\/[-A-Za-z0-9+&@#\\/%?=~_|!:,.;]*[-A-Za-z0-9+&@#\\/%=~_|]"
    },
    "date": {
      "type": "date"
    },
    "summary": {
      "type": "text"
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "inversedBy": "awards"
    },
    "body": {
      "type": "richtext"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "experiences": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::experience.experience",
      "mappedBy": "awards"
    }
  }
}
