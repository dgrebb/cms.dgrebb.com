{
  "name": "dgrebb.com",
  "version": "4.2.2",
  "private": true,
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --compiler-warnings \"css-unused-selector:ignore\"",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch --compiler-warnings \"css-unused-selector:ignore\"",
    "prettier": "prettier",
    "lint.fix": "prettier --write .",
    "baseline": "mv -f ./.report/bundle/current/* ./.report/bundle/previous",
    "bundle-stats": "bundle-stats",
    "lint": "prettier --check . && eslint .",
    "dev.l": "vite --config vite.local.config.js dev",
    "dev.s": "vite --config vite.local.config.js dev",
    "dev.p": "vite --config vite.local.config.js dev",
    "dev": "vite --config vite.config.js dev",
    "build.l": "WARN_ONLY=true vite --config vite.local.config.js build",
    "build.s": "svelte-kit sync && vite --config vite.config.js build",
    "build.p": "svelte-kit sync && vite --config vite.config.js build",
    "build": "svelte-kit sync && vite --config vite.config.js build",
    "preview": "vite --config vite.config.js preview",
    "preview.local": "vite --config vite.local.config.js preview",
    "postbuild.l": "npx -y -q svelte-sitemap -i '404.html' -i '/404/index.html' -i 'shingle.html' -i '/shingle/index.html' -i '/posts/category/index.html' -i '**/cv/*/**' -t --attribution=false --domain https://local.dgrebb.com",
    "postbuild.s": "npx -y -q svelte-sitemap -i '404.html' -i '/404/index.html' -i 'shingle.html' -i '/shingle/index.html' -i '/cv/*/**' -t --attribution=false --domain https://stg.dgrebb.com",
    "postbuild.p": "npx -y -q svelte-sitemap -i '404.html' -i '/404/index.html' -i 'shingle.html' -i '/shingle/index.html' -i '/cv/*/**' -t --attribution=false --domain https://www.dgrebb.com",
    "postbuild": "npx -y -q svelte-sitemap -i '404.html' -i '/404/index.html' -i 'shingle.html' -i '/shingle/index.html' -i '/cv/*/**' -t --attribution=false --domain https://www.dgrebb.com",
    "format": "prettier --write .",
    "test.all": "pnpm run test.integration && pnpm run test.unit",
    "test.integration": "playwright test",
    "test.unit": "vitest",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@accuser/svelte-plausible-analytics": "dgrebb/svelte-plausible-analytics#downstream/feature-support-custom-pageview-properties",
    "@iconify-json/entypo-social": "^1.1.9",
    "@iconify-json/gg": "^1.1.10",
    "@iconify-json/icomoon-free": "^1.1.9",
    "@iconify-json/icon-park-outline": "^1.1.16",
    "@iconify-json/mdi": "^1.1.68",
    "@iconify-json/ph": "^1.1.14",
    "@iconify-json/simple-icons": "^1.1.114",
    "@iconify-json/typcn": "^1.1.10",
    "@playwright/test": "^1.46.1",
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.24",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "highlight.js": "^11.10.0",
    "html-minifier-terser": "^7.2.0",
    "marked": "^13.0.3",
    "marked-alert": "^2.0.1",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.2.0",
    "postcss-nesting": "^13.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-svelte": "^3.2.6",
    "rollup-plugin-bundle-stats": "^4.14.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "slugger": "^1.0.1",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-highlight": "7.7.0",
    "svelte-meta-tags": "^3.1.3",
    "svelte-sitemap": "^2.6.0",
    "typescript": "^5.5.4",
    "unplugin-icons": "^0.19.2",
    "vite": "^5.4.2",
    "vite-plugin-progress": "^0.0.7",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=16.0.0 <=20.x.x",
    "npm": ">=6.0.0",
    "pnpm": ">= 8.14.1"
  },
  "type": "module",
  "pnpm": {
    "patchedDependencies": {
      "svelte-highlight": "patches/svelte-highlight.patch",
      "@sveltejs/kit": "patches/@sveltejs__kit.patch"
    }
  }
}
