{
  "kind": "collectionType",
  "collectionName": "skills",
  "info": {
    "singularName": "skill",
    "pluralName": "skills",
    "displayName": "ðŸ‘” Skillz",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "proficiency": {
      "type": "integer",
      "max": 10,
      "required": true
    },
    "graphColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color",
      "required": true
    },
    "iconColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "summary": {
      "type": "text"
    },
    "experiences": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::experience.experience",
      "mappedBy": "skills"
    },
    "organizations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "mappedBy": "skills"
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "skills"
    },
    "classifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::classification.classification",
      "mappedBy": "skills"
    },
    "industries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::industry.industry",
      "mappedBy": "skills"
    },
    "certifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::certification.certification",
      "mappedBy": "skills"
    },
    "body": {
      "type": "richtext"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
